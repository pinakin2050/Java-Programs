<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="p1.Student" table="STUDENT_HIBERNATE">
		<id name="roll" type="string" column="ROLL_NO"></id>
		<property name="name">
			<column name="NAME" />
		</property>
	</class>
</hibernate-mapping>