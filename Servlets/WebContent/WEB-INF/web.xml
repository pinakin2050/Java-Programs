<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
	<display-name>Servlets</display-name>
	<servlet>
		<description>
		</description>
		<display-name>Hello</display-name>
		<servlet-name>Hello</servlet-name>
		<servlet-class>hello.Hello</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>FirstServletPage</display-name>
		<servlet-name>FirstServletPage</servlet-name>
		<servlet-class>P1.FirstServletPage</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>DynamicHtml</display-name>
		<servlet-name>DynamicHtml</servlet-name>
		<servlet-class>P1.DynamicHtml</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>GetResult</display-name>
		<servlet-name>GetResult</servlet-name>
		<servlet-class>P1.GetResult</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Hello</servlet-name>
		<url-pattern>/Hello</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>FirstServletPage</servlet-name>
		<url-pattern>/FirstServletPage</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DynamicHtml</servlet-name>
		<url-pattern>/DynamicHtml</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>GetResult</servlet-name>
		<url-pattern>/GetResult</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
</web-app>
